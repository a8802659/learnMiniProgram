<!--pages/home/childCpns/f-recommend/f-recommend.wxml-->
<view class="recommend">
  <block wx:for="{{list}}" wx:key="index">
    <view class="recommend-item">
      <image src="{{item.image}}" class="image" bind:load="handleImageLoad"></image>
      <view class="title">{{item.title}}</view>
    </view>
  </block>
</view>
