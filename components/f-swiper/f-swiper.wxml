<!--components/f-swiper/f-swiper.wxml-->
<swiper class="swiper"
        autoplay
        circular
        interval="2000"
        indicator-dots
        indicator-active-color='#ff5777'>
  <block wx:for="{{list}}" wx:key="index">
    <swiper-item class="swiper-item">
      <image class="image" src="{{item.image}}" mode="widthFix"></image>
    </swiper-item>
  </block>
</swiper>
